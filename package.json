    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "microbundle": "^0.7.0",
    "prettier": "^1.15.2"
  },
  "keywords": [
    "vue",
    "router",
    "routing",
    "webpack"
  ]
