{
  "name": "@badrap/routdir",
  "version": "0.0.3",
  "description": "Filesystem based route generation for Webpack + vue-router",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.js",
  "repository": "badrap/routdir",
  "author": "Joachim Viide <jviide@iki.fi>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ignore-pattern examples .",
